<?xml version="1.0" encoding="utf-8"?>
<LinearLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/height_30dp"
    android:background="#ffffff"
    android:orientation="horizontal"
    >


    <TextView
        android:id="@+id/tv_result_detail_companylist_num_parent"
        android:layout_width="0dp"
        android:layout_height="@dimen/height_30dp"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="@dimen/txt_sub"
        android:textColor="@color/textColor_deep"
        />

    <TextView
        android:id="@+id/tv_result_detail_companylist_company_parent"
        android:layout_width="0dp"
        android:layout_height="@dimen/height_30dp"
        android:layout_weight="3"
        android:gravity="center"
        android:textSize="@dimen/txt_sub"
        android:textColor="@color/textColor_deep"
        android:singleLine="true"
        />

    <TextView
        android:id="@+id/tv_result_detail_companylist_price_parent"
        android:layout_width="0dp"
        android:layout_height="@dimen/height_30dp"
        android:layout_weight="3"
        android:gravity="center"
        android:textSize="@dimen/txt_sub"
        android:textColor="@color/textColor_deep"
        android:singleLine="true"
        />

    <TextView
        android:id="@+id/tv_result_detail_companylist_percent_parent"
        android:layout_width="0dp"
        android:layout_height="@dimen/height_30dp"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="@dimen/txt_sub"
        android:textColor="@color/textColor_deep"
        android:singleLine="true"
        />



</LinearLayout>