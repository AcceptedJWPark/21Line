<?xml version="1.0" encoding="utf-8"?>



    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
    android:background="#ffffff"
    >

        <android.support.v7.widget.Toolbar
            android:id="@+id/customerservice_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_40dp"
            android:contentInsetLeft="0dp"
            android:contentInsetStart="0dp"
            android:fitsSystemWindows="true"
            android:padding="0dp"
            app:contentInsetLeft="0dp"
            app:contentInsetStart="0dp">

            <include
                layout="@layout/toolbar"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </android.support.v7.widget.Toolbar>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_30dp"
                android:orientation="horizontal"
                android:background="@drawable/bgr_search_inputbox"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="@dimen/height_10dp"
                android:gravity="center"
                >

                <EditText
                    android:id="@+id/et_notice_search"
                    android:layout_width="0dp"
                    android:layout_weight="7"
                    android:layout_height="20dp"
                    android:background="#ffffff"
                    android:layout_margin="3dp"
                    android:paddingLeft="10dp"
                    android:singleLine="true"
                    android:textSize="@dimen/txt_sub"
                    android:hint="검색"
                    />

                <ImageView
                    android:id="@+id/iv_notice_search"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/height_15dp"
                    android:layout_weight="1"
                    android:src="@drawable/search_sample"
                    android:tint="@color/colorPrimaryDark"
                    />

            </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#d7d7d7"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="@dimen/height_10dp"
                    />


                    <ListView
                        android:id="@+id/lv_notice_cs"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:dividerHeight="0dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:divider="#ffffff"
                        android:clickable="true"
                        />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d7d7d7"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="@dimen/height_10dp"
                />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_30dp"
                android:orientation="horizontal"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="@dimen/height_10dp"
                >

                <TextView
                    android:id="@+id/tv_series_pre"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:text="이전"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    />
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />

                <TextView
                    android:id="@+id/tv_series1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="1"
                    />
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />

                <TextView
                    android:id="@+id/tv_series2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="2"
                    />
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />

                <TextView
                    android:id="@+id/tv_series3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="3"
                    />
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />

                <TextView
                    android:id="@+id/tv_series4"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="4"
                    />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />



                <TextView
                    android:id="@+id/tv_series5"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="5"
                    />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />

                <TextView
                    android:id="@+id/tv_series6"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="6"
                    />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />

                <TextView
                    android:id="@+id/tv_series7"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="7"
                    />
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />



                <TextView
                    android:id="@+id/tv_series8"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="8"
                    />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />


                <TextView
                    android:id="@+id/tv_series9"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="9"
                    />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />

                <TextView
                    android:id="@+id/tv_series10"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/textColor_soft"
                    android:text="10"
                    />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#eeeeee"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    />



                <TextView
                    android:id="@+id/tv_series_next"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:gravity="center"
                    android:textSize="@dimen/txt_sub"
                    android:textColor="@color/colorPrimaryDark"
                    android:text="다음"
                    />


            </LinearLayout>


        </LinearLayout>

    </LinearLayout>
