<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#ffffff"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30dp"
            android:background="@drawable/bgr_btn_clicked">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:text="낙찰업체 정보"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/txt_main"
                android:textStyle="bold" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="구분"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/tv_result_com_type"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="업체명"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/tv_result_com_name"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="사업자번호"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/tv_result_com_biz_no"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"
                android:background="@drawable/bgr_bid_detail_title1"
                android:text="대표자"
                />


            <TextView
                android:id="@+id/tv_result_com_ceo_name"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:minHeight="@dimen/height_30dp"
                android:gravity="center|left"
                android:paddingLeft="10dp"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_deep"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:lineSpacingMultiplier="1.5"
                android:includeFontPadding="false"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                />



        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="전화번호"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/tv_result_com_phone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:includeFontPadding="false"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_phone"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="주소"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/tv_result_com_addr"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:includeFontPadding="false"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="투찰 금액"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/tv_result_com_joinPrice"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:includeFontPadding="false"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="투찰율"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"

                />

            <TextView
                android:id="@+id/tv_result_com_joinRate"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title1"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="사정율"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"

                />

            <TextView
                android:id="@+id/tv_result_com_RateRange"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens1"
                android:gravity="center|left"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_30dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/bgr_bid_detail_title2"
                android:gravity="center|left"
                android:minHeight="@dimen/height_30dp"
                android:paddingLeft="10dp"
                android:text="기초대비"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/tv_result_com_BPercent"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:background="@drawable/bgr_bid_detail_contens2"
                android:gravity="center|left"
                android:lineSpacingMultiplier="1.5"
                android:minHeight="@dimen/height_30dp"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_sub" />
        </LinearLayout>


    </LinearLayout>


</LinearLayout>







