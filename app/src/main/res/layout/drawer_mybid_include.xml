<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >


        <TextView
            android:id="@+id/tv_mybid_mybid"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:text="내 서류함"
            android:gravity="center_vertical"
            android:textSize="@dimen/txt_main"
            android:textColor="@color/textColor_deep"
            android:background="@drawable/bgr_dl_list"
            android:paddingLeft="@dimen/height_10dp"
            />



        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:background="@drawable/bgr_dl_list">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignBaseline="@+id/alert_Rev_Can_dl_dl"
                android:layout_alignBottom="@+id/alert_Rev_Can_dl_dl"
                android:layout_alignParentStart="true"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/height_10dp"
                android:text="스케줄러"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main" />

            <TextView
                android:id="@+id/alert_Rev_Can_dl_dl"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/height_20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_highlight_ngt"
                android:paddingRight="10dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:background="@drawable/bgr_dl_list">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="분석의뢰함"
            android:gravity="center_vertical"
            android:textColor="@color/textColor_deep"
            android:textSize="@dimen/txt_main"
            android:paddingLeft="@dimen/height_10dp"
            />

            <TextView
                android:id="@+id/alert_Req_dl"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/height_20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_highlight_ngt"
                android:paddingRight="10dp"
                />

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:text="나의 메모"
            android:gravity="center_vertical"
            android:textSize="@dimen/txt_main"
            android:textColor="@color/textColor_deep"
            android:background="@drawable/bgr_dl_list"
            android:paddingLeft="@dimen/height_10dp"
            />



    </LinearLayout>
</LinearLayout>
