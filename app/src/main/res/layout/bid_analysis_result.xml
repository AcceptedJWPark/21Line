<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:gravity="center"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <LinearLayout
        android:id="@+id/ll_sortingbox_bid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#ffffff"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_40dp"
            android:background="@drawable/bgr_btn_clicked"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="예가산출 결과"
                android:textSize="@dimen/txt_main"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:textStyle="bold"
                android:textColor="@color/colorPrimaryDark"
                />

            <ImageView
                android:id="@+id/iv_cancel_analysisresult"
                android:layout_width="@dimen/height_15dp"
                android:layout_height="@dimen/height_15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@drawable/icon_close2"
                android:clickable="true"
                android:focusable="false"
                android:focusableInTouchMode="false"
                />

        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_200dp"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="10"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_weight="1"
                    android:background="#f5f5f5"
                    android:gravity="center"
                    android:text="선택 번호"
                    android:textColor="@color/textColor_deep"
                    android:textSize="@dimen/txt_main" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#dcdcdc"/>


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:orientation="vertical"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:id="@+id/tv_analResult_num1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="①"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"

                            />

                        <TextView
                            android:id="@+id/tv_analResult_money1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3.5"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="24,754,616원"
                            />

                        <TextView
                            android:id="@+id/tv_analResult_rate1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="-1.7414%"
                            />


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#f0f0f0" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:id="@+id/tv_analResult_num2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="②"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            />

                        <TextView
                            android:id="@+id/tv_analResult_money2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3.5"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="23,754,616원"
                            />

                        <TextView
                            android:id="@+id/tv_analResult_rate2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="-1.7414%"
                            />



                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#f0f0f0" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:id="@+id/tv_analResult_num3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="③"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            />
                        <TextView
                            android:id="@+id/tv_analResult_money3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3.5"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="24,754,616원"
                            />

                        <TextView
                            android:id="@+id/tv_analResult_rate3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="-1.7414%"
                            />


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#f0f0f0" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:id="@+id/tv_analResult_num4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="④"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            />
                        <TextView
                            android:id="@+id/tv_analResult_money4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3.5"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="24,754,616원"
                            />

                        <TextView
                            android:id="@+id/tv_analResult_rate4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:textColor="@color/textColor_soft"
                            android:textSize="@dimen/txt_sub"
                            android:gravity="center"
                            android:text="-1.7414%"
                            />


                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#dcdcdc" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="예정 금액"
                    android:gravity="center"
                    android:background="#f5f5f5"
                    android:textColor="@color/textColor_deep"
                    android:textSize="@dimen/txt_main"
                    />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#dcdcdc"/>


                        <TextView
                            android:id="@+id/tv_analResult_estMoney"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:text="24,859,942원"
                            android:textColor="@color/textColor_highlight_ngt"
                            android:textSize="@dimen/txt_main"
                            android:gravity="center"

                            />




            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#dcdcdc" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="투찰 금액"
                    android:background="#f5f5f5"
                    android:textColor="@color/textColor_deep"
                    android:textSize="@dimen/txt_main" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#dcdcdc"/>

                <TextView
                    android:id="@+id/tv_analResult_tuchalMoney"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:text="24,859,942원"
                    android:textColor="@color/textColor_highlight_pgt"
                    android:textSize="@dimen/txt_main"
                    android:gravity="center"
                    android:textStyle="bold"

                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#dcdcdc"
                />

        </LinearLayout>




        <Button
            android:id="@+id/btn_analResult_saveMemo"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_20dp"
            android:background="@drawable/bgr_bigbtn"
            android:text="메모함에 저장"
            android:textColor="#ffffff"
            android:textSize="@dimen/txt_main"
            android:layout_marginTop="@dimen/height_10dp"
            android:layout_marginBottom="@dimen/height_10dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#dcdcdc"
            />


        </LinearLayout>

    </LinearLayout>


