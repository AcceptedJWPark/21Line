<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical">





        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="발주처정보입력"
                    android:gravity="center|left"
                    android:textSize="11dp"
                    android:background="@drawable/bgr_btn_clicked"
                    android:textStyle="bold"
                    android:textColor="@color/colorPrimaryDark"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="회사명"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_company_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="대표자"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_ceo_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="사업자번호"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_companyNo1_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="0.3"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:inputType="number"
                    android:textSize="11dp"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.05"
                    android:gravity="center"
                    android:textColor="@color/textColor_deep"
                    android:text="-"
                    />
                <EditText
                    android:id="@+id/et_companyNo2_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="0.3"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:inputType="number"
                    android:textSize="11dp"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.05"
                    android:gravity="center"
                    android:textColor="@color/textColor_deep"
                    android:text="-"
                    />
                <EditText
                    android:id="@+id/et_companyNo3_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="0.3"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp"
                    android:inputType="number"
                    android:layout_marginRight="10dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="주소"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"

                    android:layout_marginRight="10dp"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.72"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.03"/>

                <Button
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.25"
                    android:background="@drawable/bgr_smallbtn"
                    android:text="주소 검색"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="11dp"
                    android:textStyle="bold"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/bgr_joinbox"
                        android:paddingLeft="10dp"
                        android:textSize="11dp" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="업태"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_business_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="종목"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_businessType_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="담당자명"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_charge_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="입찰 또는 결제 담당자 (숫자만 입력)"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="전화번호"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_tel_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="(숫자만 입력)"
                    android:paddingLeft="10dp"
                    android:inputType="number"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="핸드폰"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_phone_orderUser"
                    android:inputType="number"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="담당자 번호 (숫자만 입력)"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="Fax"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_fax_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="안내문 발송 (숫자만 입력)"
                    android:inputType="number"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="홈페이지"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_homepage_orderUser"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>
        </LinearLayout>

</LinearLayout>
