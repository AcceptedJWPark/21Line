<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <RelativeLayout
            android:id="@+id/rl_cs_notice_dl_contents1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:background="@drawable/bgr_dl_list"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_50dp"
                android:layout_alignParentLeft="true"
                android:text="공지사항"
                android:gravity="center_vertical"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"
                android:paddingLeft="10dp"
                />

        </RelativeLayout>





        <RelativeLayout
            android:id="@+id/rl_cs_question_dl_contents2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:background="@drawable/bgr_dl_list"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_50dp"
                android:layout_alignParentLeft="true"
                android:text="질문/답변"
                android:gravity="center_vertical"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"
                android:paddingLeft="10dp"
                />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_cs_call_dl_contents3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:background="@drawable/bgr_dl_list"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_50dp"
                android:layout_alignParentLeft="true"
                android:text="고객센터 (전화걸기)"
                android:gravity="center_vertical"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"
                android:paddingLeft="10dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_cs_kakao_dl_contents4"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:background="@drawable/bgr_dl_list"
            android:visibility="gone"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_50dp"
                android:layout_alignParentLeft="true"
                android:text="카카오톡 플러스"
                android:gravity="center_vertical"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"
                android:paddingLeft="10dp"
                android:visibility="gone"
                />

        </RelativeLayout>

        <RelativeLayout
        android:id="@+id/rl_cs_education_dl_contents4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_50dp"
        android:background="@drawable/bgr_dl_list"
        >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:layout_alignParentLeft="true"
            android:text="입찰 교육 신청"
            android:gravity="center_vertical"
            android:textColor="@color/textColor_deep"
            android:textSize="@dimen/txt_main"
            android:paddingLeft="10dp"
            />

    </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_cs_mas_dl_contents5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_50dp"
            android:background="@drawable/bgr_dl_list"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_50dp"
                android:layout_alignParentLeft="true"
                android:text="공공조달인증 상담신청"
                android:gravity="center_vertical"
                android:textColor="@color/textColor_deep"
                android:textSize="@dimen/txt_main"
                android:paddingLeft="10dp"
                />

        </RelativeLayout>



    </LinearLayout>
</LinearLayout>
