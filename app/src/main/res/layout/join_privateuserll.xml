<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_privateUser_Join2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="개인정보입력"
                    android:gravity="center|left"
                    android:textSize="11dp"
                    android:background="@drawable/bgr_btn_clicked"
                    android:textStyle="bold"
                    android:textColor="@color/colorPrimaryDark"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="성 명"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="생년월일"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="예) 19800101 - 8자리 입력"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="전화번호"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="(숫자만 입력)"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="핸드폰"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="담당자 번호 (숫자만 입력)"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="Fax"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:hint="안내문 발송 (숫자만 입력)"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/bgr_phoneinputbox"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.3"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:text="홈페이지"
                    android:textColor="@color/textColor_deep"
                    android:textSize="11dp"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/bgr_joinbox"
                    android:paddingLeft="10dp"
                    android:textSize="11dp" />

            </LinearLayout>
        </LinearLayout>

</LinearLayout>
