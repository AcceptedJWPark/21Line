<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#ffffff"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30dp"
            android:text="예가변동폭 사정율"
            android:gravity="center|left"
            android:background="@drawable/bgr_btn_clicked"
            android:textSize="@dimen/txt_main"
            android:textColor="@color/colorPrimaryDark"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/height_10dp"
            />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/height_40dp"
            android:orientation="horizontal"
            >



            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:minHeight="@dimen/height_40dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_main"
                android:textColor="@color/textColor_deep"
                android:background="#f5f5f5"
                android:text="구분"
                />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#dcdcdc"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:minHeight="@dimen/height_40dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_main"
                android:textColor="@color/textColor_deep"
                android:background="#f5f5f5"
                android:text="발주처별"
                />


            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#dcdcdc"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:minHeight="@dimen/height_40dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_main"
                android:textColor="@color/textColor_deep"
                android:background="#f5f5f5"
                android:text="구매/공사/용역"
                />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#dcdcdc"
                />


            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:minHeight="@dimen/height_40dp"
                android:textSize="@dimen/txt_sub"
                android:text="업종별"
                android:gravity="center"
                android:textColor="@color/textColor_deep"
                android:background="#f5f5f5"
                />


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#dcdcdc" />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#dcdcdc" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30dp"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_main"
                android:textColor="@color/textColor_deep"
                android:text="최고 예비가격"
                />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#dcdcdc"
                />

            <TextView
                android:id="@+id/tv_bidAnalysis_orderMaxPrice"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_soft"
                android:text="102.98801%"
                />


            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#f0f0f0"
                />

            <TextView
                android:id="@+id/tv_bidAnalysis_kindMaxPrice"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_soft"
                android:text="97.00904%"
                />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#f0f0f0"
                />


            <TextView
                android:id="@+id/tv_bidAnalysis_upcodeMaxPrice"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textSize="@dimen/txt_sub"
                android:gravity="center"
                android:textColor="@color/textColor_soft"
                android:text="98.8211%"
                />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f0f0f0" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30dp"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_main"
                android:textColor="@color/textColor_deep"
                android:text="최저 예비가격"
                />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#dcdcdc"
                />

            <TextView
                android:id="@+id/tv_bidAnalysis_orderMinPrice"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_soft"
                android:text="102.99111%"
                />


            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#f0f0f0"
                />

            <TextView
                android:id="@+id/tv_bidAnalysis_kindMinPrice"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_soft"
                android:text="-"
                />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#f0f0f0"
                />


            <TextView
                android:id="@+id/tv_bidAnalysis_upcodeMinPrice"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textSize="@dimen/txt_sub"
                android:gravity="center"
                android:textColor="@color/textColor_soft"
                android:text="98.8513%"
                />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f0f0f0" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30dp"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_main"
                android:textColor="@color/textColor_deep"
                android:text="평균 예비가격"
                />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#dcdcdc"
                />

            <TextView
                android:id="@+id/tv_bidAnalysis_orderAvgPrice"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_soft"
                android:text="102.96808%"
                />


            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#f0f0f0"
                />

            <TextView
                android:id="@+id/tv_bidAnalysis_kindAvgPrice"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/txt_sub"
                android:textColor="@color/textColor_soft"
                android:text="97.1078%"
                />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#f0f0f0"
                />


            <TextView
                android:id="@+id/tv_bidAnalysis_upcodeAvgPrice"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textSize="@dimen/txt_sub"
                android:gravity="center"
                android:textColor="@color/textColor_soft"
                android:text="99.7611%"
                />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#dcdcdc"
            android:layout_marginBottom="@dimen/height_10dp" />
    </LinearLayout>

        </LinearLayout>


